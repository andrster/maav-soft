/**
 * ZCM type for inertial ground truth from sim
 */
struct groundtruth_inertial_t
{
	int64_t utime;

	// State variables
	quaternion_t attitude;
	// LEGEND: [gt_pos_x, gt_pos_y, gt_pos_z]
	vector3_t position;
	// LEGEND: [gt_vel_x, gt_vel_y, gt_vel_z]
	vector3_t velocity;
	// LEGEND: [gt_ω_x, gt_ω_y, gt_ω_z]
	vector3_t angular_velocity;
	// LEGEND: [gt_acc_x, gt_acc_y, gt_acc_z]
	vector3_t acceleration;
}
